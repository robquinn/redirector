{
  "name": "redirector",
  "version": "1.0.0",
  "private": true,
  "description": "An app that redirects users to a company portal, and then automatically authenticates them",
  "keywords": [
    "roster",
    "mechanics",
    "russ",
    "lyon",
    "sotheby's",
    "international",
    "realty",
    "google",
    "apps",
    "script"
  ],
  "license": "MIT",
  "author": {
    "name": "Robert Quinn"
  },
  "module": "commonjs",
  "scripts": {
    "test:unit": "jest --forceExit --detectOpenHandles -c jest-unit.config.ts test/__tests__/unit",
    "test:integration": "jest --forceExit --detectOpenHandles -c jest-integration.config.ts ./test/__tests__/integration.test.ts",
    "generate": "run-s prisma:generate nexus:generate graphql-codegen",
    "nexus:generate": "ts-node --project tsconfig/tsconfig.server.json src/server/graphql/nexus.schema.ts",
    "graphql-codegen": "graphql-codegen",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "sort": "npx sort-package-json",
    "eslint:fix": "eslint -c ./.eslintrc.js --ignore-path ./.eslintignore --ext ts,tsx --quiet --fix .",
    "prettier:write": "prettier --write .",
    "lint-staged": "lint-staged",
    "reset-db:dev": "scripts/reset-docker-db/reset-db-dev.sh",
    "reset-db:prod": "scripts/reset-docker-db/reset-db-prod.sh",
    "rimraf:dist": "rimraf dist",
    "webpack:validate": "cross-env NODE_OPTIONS=--max_old_space_size=16384 NODE_ENV=development webpack configtest webpack.config.js",
    "webpack:dev": "cross-env NODE_OPTIONS=--max_old_space_size=16384 NODE_ENV=development webpack -c webpack.config.js",
    "webpack:prod": "cross-env NODE_OPTIONS=--max_old_space_size=16384 NODE_ENV=production webpack -c webpack.config.js",
    "build": "run-s build:prod",
    "build:dev": "run-s rimraf:dist generate webpack:dev",
    "build:prod": "run-s rimraf:dist generate webpack:prod",
    "start": "node ./dist/server/bundle.js",
    "dev": "run-s build:dev start",
    "prod": "run-s build:prod start",
    "env:example": "sh ./scripts/env/example.sh",
    "husky:install": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@prisma/client": "^4.16.1",
    "@urql/core": "^4.0.10",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "graphql-tag": "^2.12.6",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.8",
    "prisma": "^4.16.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@jest/globals": "^29.5.0",
    "@nexus/typescript-language-service": "^0.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jest-puppeteer": "^9.0.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "puppeteer": "^20.7.3",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rimraf": "^5.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "tree-kill": "^1.2.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x",
    "pnpm": "8.x"
  },
  "pnpm": {
    "overrides": {
      "semver@<7.5.2": ">=7.5.2"
    }
  }
}
